    "rollup": "2.3.3",
    "rollup-plugin-analyzer": "3.2.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-progress": "1.1.1",
    "rollup-plugin-size-snapshot": "0.11.0",
    "rollup-plugin-sizes": "1.0.2",
